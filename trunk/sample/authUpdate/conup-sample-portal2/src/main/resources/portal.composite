<?xml version="1.0" encoding="UTF-8"?>
		
	<!-- requires="trace" policySets="tracePolicySet" policySets="traceImplPolicySet traceBindingPolicySet"-->
		
<composite	xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		xmlns:tuscany="http://tuscany.apache.org/xmlns/sca/1.1"
		xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		targetNamespace="http://conup"
		name="conup-sample-portal2"
		 >
        
	<component name="Portal2Component">
		<implementation.java class="cn.edu.nju.moon.conup.sample.portal2.services.PortalServiceImpl"/>
		<service name="PortalService" requires="trace buffer" policySets="traceBindingPolicySet bufferBindingPolicySet">
			<tuscany:binding.jsonrpc uri="http://10.0.2.15:8184/Portal2Component/PortalService" />
		</service>
		
		<reference name="procService" requires="trace buffer" policySets="traceBindingPolicySet bufferBindingPolicySet">
			<tuscany:binding.jsonrpc uri="http://10.0.2.15:8183/Proc2Component/ProcService" />
		</reference>
		<reference name="tokenService"  requires="trace buffer" policySets="traceBindingPolicySet bufferBindingPolicySet">
				<tuscany:binding.jsonrpc uri="http://10.0.2.15:8082/AuthComponent/TokenService" />
		</reference>
	</component>
	


</composite>
