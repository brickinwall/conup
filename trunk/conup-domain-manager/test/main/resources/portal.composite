<?xml version="1.0" encoding="UTF-8"?>
<composite	xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		xmlns:tuscany="http://tuscany.apache.org/xmlns/sca/1.1"
		xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		targetNamespace="http://store"
		name="portal-node"
		 >
        
	<component name="PortalComponent">
		<implementation.java class="cn.edu.nju.moon.vc.portal.services.PortalServiceImpl"/>
		<service name="PortalService" requires="trace keeper" policySets="traceBindingPolicySet keeperBindingPolicySet">
			<tuscany:binding.jsonrpc uri="http://10.0.2.15:8084/PortalComponent/PortalService" />
		</service>
		<reference name="tokenService"  requires="trace keeper" policySets="traceBindingPolicySet keeperBindingPolicySet">
			<tuscany:binding.jsonrpc uri="http://10.0.2.15:8081/AuthComponent/TokenService" />
		</reference>
		<reference name="procService" requires="trace keeper" policySets="traceBindingPolicySet keeperBindingPolicySet">
			<tuscany:binding.jsonrpc uri="http://10.0.2.15:8083/ProcComponent/ProcService" />
		</reference>
	</component>
	


</composite>
